<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RobotoMedium">
        <item name="android:fontFamily">sec-roboto-medium</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="popup_alert_dialog_button" parent="RobotoMedium">
        <item name="android:fontFamily">sec</item>
        <item name="android:textFontWeight">600</item>
        <item name="android:textColor">#057C05</item>
        <item name="android:textSize">10dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="backgroundTint">#E3EAE1</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="cornerRadius">7dp</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
    </style>

    <style name="custom_style" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textSize">25dp</item>
        <item name="backgroundTint">#EFE9E9</item>
        <item name="android:textColor">#51832B</item>
    </style>
</resources>